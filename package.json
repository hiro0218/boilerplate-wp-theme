{
  "name": "boilerplate-wp-theme",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development",
    "lint:js": "eslint --ext .js ./src/script/**",
    "lint:sass": "stylelint ./src/style",
    "format:js": "npm run lint:js -- --fix"
  },
  "lint-staged": {
    "**/*.php": [
      "prettier --write",
      "git add"
    ],
    "**/*.js": [
      "eslint --fix",
      "eslint",
      "git add"
    ],
    "**/*.scss": [
      "npm run lint:sass",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiro0218/boilerplate-wp-theme.git"
  },
  "author": "hiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hiro0218/boilerplate-wp-theme/issues"
  },
  "homepage": "https://github.com/hiro0218/boilerplate-wp-theme#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@hiro0218/stylelint-config": "^1.1.0",
    "@prettier/plugin-php": "^0.13.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^4.2.3",
    "import-glob": "^1.5.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-mq-optimize": "^1.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
